<!DOCTYPE HTML>
<html>
    <head>
        <meta charset="utf-8">
        <link rel="shortcut icon" href="favicon.png" type="image/x-icon">
        <title>{% block title %}Air base{% endblock %}</title>
        <link rel="stylesheet" href="node_modules/bootstrap/dist/css/bootstrap.min.css">
        <link rel="stylesheet" href="node_modules/font-awesome/css/font-awesome.min.css">
        <link rel="stylesheet" href="assets/css/style.css">
        <link rel="stylesheet" href="assets/css/form.css">
    </head>
    <body>
        {% block header %}
            {% include 'back/includes/nav.html.twig' %}
        {% endblock header %}

        <div class="content-back">
            {% block content %}
                <div class="container">
                    {% block new %}
                    {% endblock new %}
                </div>
            {% endblock content %}
        </div>

        {% block footer %}
            <script src="node_modules/jquery/dist/jquery.min.js" type="text/javascript"></script>
            <script src="js_feature/config/parameter.js" type="text/javascript"></script>
            <script src="js_feature/back/flight/crud.js" type="text/javascript"></script>
        {% endblock footer %}
    </body>
</html>
