<!DOCTYPE HTML>
<html>
	<head>
		<meta charset="utf-8">
		<title>{% block title %}Air base{% endblock %}</title>
        <link rel="shortcut icon" href="favicon.png" type="image/x-icon">
		<link rel="stylesheet" href="node_modules/bootstrap/dist/css/bootstrap.min.css">
        <link rel="stylesheet" href="node_modules/font-awesome/css/font-awesome.min.css">
        <link rel="stylesheet" href="assets/css/style.css">
	</head>
	<body>
		{% block header %}
			{% include 'front/includes/nav.html.twig' %}
        {% endblock header %}

		<div class="container">
			<div class="content">
				{% block content %}
				{% endblock content %}
			</div>
		</div>

		{% block footer %}
			<script src="node_modules/jquery/dist/jquery.min.js" type="text/javascript"></script>
			<script src="js_feature/config/parameter.js" type="text/javascript"></script>
			<script src="js_feature/front/flight/list.js" type="text/javascript"></script>
			<script src="assets/js/app.js" type="text/javascript"></script>
		{% endblock footer %}
	</body>
</html>
